{
    "version": 2,
    "builds": [
      { "src": "api/contact.js", "use": "@vercel/node" },
      { "src": "api/contact-save.js", "use": "@vercel/node" },
      { "src": "api/contact-get.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/contact/save", "dest": "/api/contact-save.js" },
      {
        "src": "/contact/get",
        "dest": "/api/contact-get.js"
      },
      { "src": "/contact", "dest": "/api/contact.js" }
    ]
  }
  